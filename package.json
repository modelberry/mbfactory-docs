{
  "name": "@modelberry/mbfactory-docs",
  "version": "1.0.0",
  "description": "Docs and examples for @modelberry/mbfactory",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pull-content": "mbfactory pull content @modelberry/plugin-contentful ./my-models-folder",
    "pull-models": "mbfactory pull models @modelberry/plugin-contentful ./my-models-folder",
    "push-content": "mbfactory push content ./examples/main-content.ts",
    "push-models": "mbfactory push models ./examples/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/modelberry/mbfactory-docs.git"
  },
  "keywords": [
    "react",
    "gatsby",
    "contentful",
    "modelberry",
    "mbfactory"
  ],
  "author": "Jacco Meijer <jacco@jaccomeijer.nl> (https://www.jaccomeijer.nl/)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/modelberry/mbfactory-docs/issues"
  },
  "homepage": "https://github.com/modelberry/mbfactory-docs#readme",
  "dependencies": {
    "@modelberry/mbfactory": "8.0.4",
    "@modelberry/plugin-contentful": "8.0.4"
  }
}
